{
    "manifest_version": 2,
    "name": "Habit tracker",
    "version": "1.0",
    "description": "A simple habit tracker",
    "permissions": [
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "dist/popup.html"
    }
}